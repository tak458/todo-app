{
  "name": "todo-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "storybook": "storybook dev -p 6006",
    "lint": "next lint -f stylish",
    "format": "prettier src/**/* --write",
    "type-check": "tsc --noEmit",
